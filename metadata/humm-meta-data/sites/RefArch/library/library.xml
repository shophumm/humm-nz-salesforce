        <online-flag>true</online-flag>
        <searchable-flag>false</searchable-flag>
        <page-attributes/>
        <custom-attributes>
            <custom-attribute attribute-id="body" xml:lang="x-default">&lt;div class="humm-checkout"&gt;&#13;
      &lt;strong&gt;You'll be redirected to the Humm website when you place an order.&lt;/strong&gt;&#13;
&lt;/div&gt;</custom-attribute>
        </custom-attributes>
    </content>

	<content content-id="humm-landing">
        <display-name xml:lang="x-default">Humm Landing</display-name>
        <online-flag>true</online-flag>
        <searchable-flag>false</searchable-flag>
        <page-attributes/>
        <custom-attributes>
            <custom-attribute attribute-id="body" xml:lang="x-default">&lt;script id="humm-landing-page" src="https://widgets.shophumm.com.au/content/scripts/landing-page.js"&gt;&lt;/script&gt;</custom-attribute>
